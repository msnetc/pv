<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.medsci.pv.report.mapper.EnterpriseClinicalMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.medsci.pv.report.entity.EnterpriseClinical">
        <id column="id" property="id" />
        <result column="enterprise_id" property="enterpriseId" />
        <result column="clinical_id" property="clinicalId" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, enterprise_id AS enterpriseId, clinical_id AS clinicalId
    </sql>

</mapper>
