<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.medsci.pv.report.mapper.SourceMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.medsci.pv.report.entity.Source">
        <id column="id" property="id" />
        <result column="company_id" property="companyId" />
        <result column="accept_report_date" property="acceptReportDate" />
        <result column="solution_number" property="solutionNumber" />
        <result column="file_class" property="fileClass" />
        <result column="category_id" property="categoryId" />
        <result column="file_source" property="fileSource" />
        <result column="remark" property="remark" />
        <result column="report_number" property="reportNumber" />
        <result column="issue" property="issue" />
        <result column="status" property="status" />
        <result column="creator_uuid" property="creatorUuid" />
        <result column="creator_name" property="creatorName" />
        <result column="created_at" property="createdAt" />
        <result column="updated_at" property="updatedAt" />
        <result column="severity" property="severity" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, company_id AS companyId, accept_report_date AS acceptReportDate, solution_number AS solutionNumber, file_class AS fileClass, category_id AS categoryId, file_source AS fileSource, remark, report_number AS reportNumber, issue, status, creator_uuid AS creatorUuid, creator_name AS creatorName, created_at AS createdAt, updated_at AS updatedAt, severity
    </sql>

</mapper>
